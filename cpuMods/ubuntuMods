>>> import tensorflow as tf
>>> hello = tf.constant('asdf')
>>> sess = tf.Session()
>>> print(sess.run(hello))
b'asdf'
>>> hello = tf.constant('Hello, TensorFlow!')
>>> sess = tf.Session()
>>> print(sess.run(hello))
b'Hello, TensorFlow!'
>>> a = tf.constant(10)
>>> b = tf.constant(32)
>>> print(sess.run(a+b))

